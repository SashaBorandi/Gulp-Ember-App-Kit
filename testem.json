{
  "parallel": 2,
  "framework": "qunit",
  "test_page": "build/test.html",
  "serve_files": [
    "coverage/**/*.js"
  ],
  "src_files": [
    "build/**/*.js"
  ],
  "before_tests": "istanbul instrument --output coverage build",
  "on_exit": "istanbul report",
  "launch_in_dev": ["PhantomJS", "Chrome"],
  "launch_in_ci": ["PhantomJS"]
}
